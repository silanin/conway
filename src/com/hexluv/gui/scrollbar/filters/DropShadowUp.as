package com.hexluv.gui.scrollbar.filters {		import flash.display.*;	import flash.filters.DropShadowFilter;		public class DropShadowUp extends Sprite {				private var _dsUp:DropShadowFilter;				public function DropShadowUp($s:Sprite):void {						//Drop shadow properties						_dsUp = new DropShadowFilter();			_dsUp.distance = 3;			_dsUp.angle = 270;			_dsUp.blurY = 5;			_dsUp.alpha = 0.2;						//Apply filter			$s.filters = [_dsUp];								}			}	}